<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.bogoods">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".page.reseller.DashboardReseller"
                android:label="@string/title_activity_dashboard_reseller"
                android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
                android:name=".page.reseller.EditProfileReseller"
                android:parentActivityName=".page.reseller.ProfileReseller"></activity>
        <activity
                android:name=".page.reseller.ProfileReseller"
                android:parentActivityName=".page.reseller.DashboardReseller"></activity>


        <activity
                android:name=".page.EditProfile"
                android:parentActivityName=".page.reseller.ProfileReseller"></activity>
        <activity
                android:name=".page.Profile"
                android:parentActivityName=".page.Dashboard"></activity>
        <activity
                android:name=".page.Dashboard"
                android:label="@string/title_activity_dashboard_seller"
                android:theme="@style/AppTheme.NoActionBar"></activity>


        <activity android:name=".page.Auth"></activity>
        <activity android:name=".page.ChooseAs"></activity>
        <activity android:name=".page.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>